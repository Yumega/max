--name=firefox \
-e TZ=Asia/Hong_Kong \
-e DISPLAY_WIDTH=1366 \
-e DISPLAY_HEIGHT=768 \
-e KEEP_APP_RUNNING=1 \
-e ENABLE_CJK_FONT=1 \
-p 5800:5800 \
-v /opt/firefox/appdata/firefox:/config:rw \
