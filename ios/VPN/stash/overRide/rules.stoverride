    url: https://raw.githubusercontent.com/Yumega/max/master/ios/VPN/stash/USA.ipcidrtext.txt
    interval: -1
  电报域名规则:
    behavior: domain-text
    url: https://raw.githubusercontent.com/Yumega/max/master/ios/VPN/stash/telegram.domaintext.txt
    interval: -1
  电报ip规则:
    behavior: ipcidr-text
    url: https://raw.githubusercontent.com/Yumega/max/master/ios/VPN/stash/telegram.ipcidrtext.txt
    interval: -1
  BBCiplayer域名规则:
    behavior: domain-text
    url: https://raw.githubusercontent.com/Yumega/max/master/ios/VPN/stash/bbciplayer.domaintext.txt
    interval: -1
  域名直连规则:
    behavior: domain-text
    url: https://raw.githubusercontent.com/Yumega/max/master/ios/VPN/stash/direct.domaintext.txt
    interval: -1
